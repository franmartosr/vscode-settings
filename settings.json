    "diffEditor.renderIndicators": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.definitionLinkOpensInPeek": true,
    "editor.fontFamily": "'Google Sans Code', Consolas, 'Courier New', monospace",
    "editor.formatOnSave": true,
    "editor.guides.bracketPairs": true,
    "editor.inlineSuggest.enabled": false,
    "editor.minimap.showSlider": "always",
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "trailing",
    "editor.showFoldingControls": "always",
    "editor.stickyScroll.enabled": false,
    "editor.suggestSelection": "first",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.wordBasedSuggestions": "off",
    "editor.wordWrap": "on",
    "eslint.useFlatConfig": false,
    "eslint.options": {
        "overrideConfigFile": "/home/francisco/Dropbox/Programacion & Software/Linterns/eslintrc.json",
    },
    "eslint.validate": [
        "javascript"
    ],
    "files.associations": {
        "*.xrd": "xml",
        "*.xln": "xml",
        "*.xrc": "xml",
        "*.xpp": "xml",
        "*.xai": "xml",
        "*.xpd": "xml",
        "*.jsb3": "json",
        ".gitignore": "gitignore",
        "*.xmd": "xml",
        "*.tcl": "tcl",
        "*.xwf": "xml",
        "*.ttfw": "tcl",
        "*.xmm": "xml",
        "*.xop": "xml",
        "*.xcgf": "xml",
        "*.xclt": "xml",
        "*.xcfm": "xml"
    },
    "files.eol": "\r\n",
    "git.autofetch": "all",
    "git.blame.editorDecoration.template": "${subject}, ${authorName} (${authorDate})",
    "git.blame.statusBarItem.enabled": true,
    "git.blame.statusBarItem.template": "${subject}, ${authorName} (${authorDate})",
    "git.branchSortOrder": "alphabetically",
    "git.confirmSync": false,
    "git.untrackedChanges": "separate",
    "git.useEditorAsCommitInput": true,
    "git.verboseCommit": true,
    "github.copilot.enable": {
        "plaintext": false,
        "markdown": true,
        "scminput": true
    },
    "github.copilot.nextEditSuggestions.allowWhitespaceOnlyChanges": false,
    "github.copilot.nextEditSuggestions.enabled": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingEmptyBraces": false,
    "javascript.preferences.quoteStyle": "single",
    "javascript.preferGoToSourceDefinition": true,
    "javascript.referencesCodeLens.enabled": true,
    "javascript.referencesCodeLens.showOnAllFunctions": true,
    "javascript.suggest.names": false,
    "js/ts.implicitProjectConfig.target": "ES5",
