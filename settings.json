    "sqlfluff.config": "/home/francisco/Dropbox/Programacion & Software/Linterns/sqlfluff.cfg",
    "svn.commit.changes.selectedAll": false,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.cursorBlinking": true,
